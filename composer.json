{
    "name": "zaruto/bird-elephant",
    "type": "library",
    "description": "A simple library to request data from the Twitter APIv2 endpoints",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Devine",
            "email": "jerk@coderjerk.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/oauth-subscriber": "^0.6.0"
    },
    "autoload": {
        "psr-4": {
            "Coderjerk\\BirdElephant\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Coderjerk\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "smolblog/oauth2-twitter": "^1.0",
        "vlucas/phpdotenv": "^5.2",
        "league/oauth1-client": "^1.10",
        "symfony/var-dumper": "^5.1",
        "phpunit/phpunit": "^9.5"
    }
}
